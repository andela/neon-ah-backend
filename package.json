{
  "name": "neon-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "undomigrate": "node_modules/.bin/sequelize db:migrate:undo:all",
    "migrate": "npm run undomigrate && node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "undoseed": "node_modules/.bin/sequelize db:seed:undo:all",
    "comp": "babel index.js -d build",
    "clear": "rimraf build && babel index.js -d build",
    "start": "npm run comp && node build/index.js",
    "dev": "nodemon --exec babel-node index.js --env",
    "migrate:test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:migrate:undo:all && cross-env NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "test": "npm run migrate:test && cross-env NODE_ENV=test nyc mocha --require @babel/register ./server/test/** --watch --exit --timeout=20000 --recursive",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cors": "^2.8.4",
    "coveralls": "^3.0.2",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.4.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "nyc": "^13.1.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "request": "^2.87.0",
    "sequelize": "^4.42.0",
    "sequelize-auto-migrations": "^1.0.3",
    "slug": "^0.9.3",
    "swagger-ui-express": "^4.0.2",
    "underscore": "^1.9.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.9",
    "rimraf": "^2.6.3",
    "sequelize-cli": "^5.4.0"
  }
}
